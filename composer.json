{
  "name": "acrolinx/sdk-php",
  "description": "PHP SDK for working with Acrolinx (without Sidebar)",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Acrolinx\\SDK\\": "src/"
    },
    "classmap": [
      "src/"
    ]
  },
  "require": {
    "php": ">=7.1",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "vlucas/phpdotenv": "^3.3"
  },
  "scripts": {
    "test": "phpunit"
  }
}
